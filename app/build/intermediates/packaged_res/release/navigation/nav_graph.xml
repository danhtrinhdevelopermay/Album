<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_photos">

    <fragment
        android:id="@+id/navigation_photos"
        android:name="com.ios18photos.app.ui.photos.PhotosFragment"
        android:label="@string/nav_photos"
        tools:layout="@layout/fragment_photos" />

    <fragment
        android:id="@+id/navigation_albums"
        android:name="com.ios18photos.app.ui.albums.AlbumsFragment"
        android:label="@string/nav_albums"
        tools:layout="@layout/fragment_albums" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.ios18photos.app.ui.search.SearchFragment"
        android:label="@string/nav_search"
        tools:layout="@layout/fragment_search" />

    <fragment
        android:id="@+id/navigation_for_you"
        android:name="com.ios18photos.app.ui.foryou.ForYouFragment"
        android:label="@string/nav_for_you"
        tools:layout="@layout/fragment_for_you" />

</navigation>